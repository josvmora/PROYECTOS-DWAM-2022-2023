<main class="col-md-8 ms-sm-auto col-lg-10 px-md-4">
   <div class="table-responsive">
       <div class="table-wrapper">
           <div class="table-title">
               <div class="row">
                   <div class="col-sm-5">
                       <h2>Costumer orders</h2>
                   </div>
                   
               </div>
           </div>
           <table class="table table-striped table-hover">
               <thead> client Orders
                   <tr>
                       <th>Order Number</th>
                       <th>Product Name</th>
                       <th>Status</th>
                       <% orders.forEach((orders) => { %>  
                        <tr>
                          <td><%= orders.orderNumber %></td>
                          <td><%= orders.productName %></td>
                          <td><%= orders.status%></td>  
                        </tr>  
                      <% }); %>
                       
                   </tr>
               </thead>
               
               
           </table>
           <div class="col-sm-5">
            <h2>orders Shipped</h2>
        </div>
           <table class="table table -striped table-hover">
            <thead> Shipped XD
                <tr>
                    <th>Order Number</th>
                    <th>Product Name</th>
                    <th>Status</th>
                    <% orders.forEach((orders) => { %>  
                     <tr>
                       <td><%= orders.orderNumber %></td>
                       <td><%= orders.productName %></td>
                       <td><%= orders.status%></td>  
                     </tr>  
                   <% }); %>
               </thead>
           </table>
           
       </div>
   </div>  
</main>
